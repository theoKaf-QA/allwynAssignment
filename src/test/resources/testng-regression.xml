<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Allwyn Assignment - Regression Test Suite" verbose="1">

    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>

    <test name="Regression Test">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>

        <classes>
            <class name="org.allwynassignment.tests.BooksTests"/>
            <class name="org.allwynassignment.tests.AuthorsTests"/>
        </classes>
    </test>
</suite>


